    "node": "22.x"
  },
  "scripts": {
    "test": "jest",
    "test:upSnap": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "npm run sitemap && next build",
