    "nock": "^13.5.4",
    "postcss-loader": "^8.1.1",
    "react-test-renderer": "^19.2.3",
    "regenerator-runtime": "^0.14.1"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "testResults",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
