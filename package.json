    "tailwindcss": "^3.4.6",
    "unified": "^11.0.5",
    "validator": "^13.15.22"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.3.1",
